{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "CacheConfiguration": {
    "SizeLimit": 500000,
    "CompactionPercentage": 0.33,
    "RefreshDelayInHours": 24,
    "RestrictedValuesCacheExpirationInHours": 168
  }
}
